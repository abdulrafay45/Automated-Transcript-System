
@{
    ViewBag.Title = "Other";
}
@model  FYPWeb.Model.Exambranch
<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/font/material-design-icons/Material-Design-Icons.woff" rel="stylesheet">
    <link rel="stylesheet" href="~/CSS/Slide Bar .css" />
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>NUML</title>
    <link rel="shortcut icon" type="image/png" href="~/Images/logo.png" />
    <link rel="stylesheet" href="~/Content/bootstrap.css" />
    <link rel="stylesheet" href="~/Content/bootstrap.css.map" />
    <link rel="stylesheet" href="~/fonts/Font/font-awesome/css/all.min.css" />
</head>

<body>

    <ul id="slide-out" class="side-nav fixed z-depth-2" style="font-size: 1.5rem">
        <li class="center no-padding">
            <div class="indigo darken-2 white-text" style="height: 180px;">
                <div class="row">
                    <img style="margin-top: 5%;" width="100" height="100" src="~/Images/Graduation Cap.png" class="responsive-img" />
                    <p style="color:black">
                        Exam Branch
                    </p>
                </div>
            </div>
        </li>
        <li id="dash_dashboard"><a class="waves-effect" style="font-size: 1.5rem" href="@Url.Action("Dashboard","ExamBranch")"><i class="fa fa-home"></i>&nbsp;&nbsp;<b>Dashboard</b></a></li>
        <li id="dash_dashboard"><a class="waves-effect" style="font-size: 1.5rem" href="@Url.Action("Other","ExamBranch")"><i class="fa fa-plus-circle"></i>&nbsp;&nbsp;<b>Incomplete DMC</b></a></li>
        <li id="dash_dashboard"><a class="waves-effect" style="font-size: 1.5rem" href="@Url.Action("Logout","Student")"><i class="fa fa-sign-out"></i>&nbsp;&nbsp;<b>Logout</b></a></li>


    </ul>

    <header>

        <nav class="indigo" role="navigation">
            <div class="nav-wrapper">
                <a data-activates="slide-out" class="button-collapse show-on-" href="#!"><img style="margin-top: 17px; margin-left: 5px;" src="https://res.cloudinary.com/dacg0wegv/image/upload/t_media_lib_thumb/v1463989873/smaller-main-logo_3_bm40iv.gif" /></a>
                <a href="#" data-activates="slide-out" class="button-collapse"><i class="mdi-navigation-menu"></i></a>
            </div>
        </nav>

    </header>

    <main>
        <div class="row">
            <div class="col s12">
                <div style="padding: 35px;" align="center" class="card">
                    <div class="row">
                        <div class="left card-title">
                            <b>Profile Detail</b>
                        </div>
                    </div>
                    <div class="row">
                        <a href="#!">
                            <div style="padding: 30px;" class="col s12 waves-effect">
                                <div class="row">
                                    <!-- Profile Picture Column -->
                                    <div class="col s12 m4">
                                        <img src="~/Images/Rectangle 1.jpg" class="responsive-img" style="border-radius: 20%;" alt="Profile Picture" />
                                    </div>
                                    <!-- End Profile Picture Column -->
                                    <!-- Profile Details Column -->
                                    <div class="col s12 m8" style="color:black">
                                        <div style="text-align:left; font-family: Minion Variable Concept;">
                                            <h2>@Model.exambranch.ExamBranchName</h2>
                                            <h3>Designation: @Model.exambranch.Designation</h3>
                                            <h3>Department: @Model.exambranch.Branch</h3>
                                        </div>
                                    </div>
                                    <!-- End Profile Details Column -->
                                </div>
                            </div>
                        </a>
                        <div class="col s1">&nbsp;</div>
                        <div class="col s1">&nbsp;</div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">


            <div class="col s12">
                <div style="padding: 35px;" align="center" class="card">


                    <div id="officialTranscript">
                        <div class="row">
                            <div class="center card-title">
                                <b style="font-family: Minion Variable Concept;">Selected Semester Transcript</b>
                            </div>
                        </div>
                        <div class="row">
                            <div style="padding: 30px;" class="col s12">
                                <div class="row">
                                    <div class="col s12 m12" style="margin-bottom:-15%; color: black; font-family: Minion Variable Concept;">
                                        @using (Html.BeginForm("Other", "ExamBranch", FormMethod.Get, new { }))
                                        {
                                            <div class="col s12">
                                                <div class="row" style="text-align: left; display: flex; align-items: center;">
                                                    <h3 style="margin-right: 10px;">System ID:</h3>
                                                    @Html.TextBoxFor(model => model.student.SystemID, new { placeholder = "NUML-.....", @class = "form-control", style = "margin-bottom: 2%; flex: 1;" })
                                                </div>
                                            </div>
                                            <div class="col s12" style="margin-bottom: 20%;">
                                                <div class="row">
                                                    <div class="col s12">
                                                        <button type="submit" class="btn waves-effect waves-light" style="margin-left: 35%; background-color: #8ea1e6; border-radius: 15px;">
                                                            Search
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>

                                <div class="row">
                                    @if (Model.resultss != null)
                                    {
                                        <div class="col s12 m12" style="color: black; font-family: Minion Variable Concept;">
                                            @using (Html.BeginForm("PrintSelected", "ExamBranch", FormMethod.Get, new { }))
                                            {
                                                <div class="col s12 m6">
                                                    <div class="row" style="display: flex; align-items: center;">
                                                        <h4 style="margin-right: 10px;">From:</h4>
                                                        @Html.TextBoxFor(model => model.student.SystemID, new { Type = "Hidden", placeholder = "Faculty", @class = "form-control", @required = "required" })
                                                        @Html.DropDownListFor(model => model.FormSem, (SelectList)ViewBag.SemesterList, "-- Select From --", new { @class = "form-control", style = "margin-bottom: 2%; flex: 1;" })
                                                    </div>
                                                </div>
                                                <div class="col s12 m6">
                                                    <div class="row" style="margin-left: 5%;display: flex; align-items: center;">
                                                        <h4 style="margin-right: 10px;">To:</h4>
                                                        @Html.DropDownListFor(model => model.ToSem, (SelectList)ViewBag.SemesterList, "-- Select To --", new { @class = "form-control", style = "margin-bottom: 2%; flex: 1;" })
                                                    </div>
                                                </div>
                                                <div class="col s12" style="margin-bottom: 20%;">
                                                    <div class="row">
                                                        <div class="col s12">
                                                            <button type="submit" class="btn waves-effect waves-light" style="margin-left: 35%; background-color: #8ea1e6; border-radius: 15px;">
                                                                Filter
                                                            </button>
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    }
                                </div>

                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>

    </main>


    <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
   
</body>




</html>


